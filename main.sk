command /region [<text>] [<text>] [<text>] [<text>]:
	trigger:
		if arg-1 is "tool":
			message "{@prefix} 영역을 선택하는 도끼가 지급되었습니다."
			give 1 wooden axe to player
			stop

		else if arg-1 is "list":
			if {region::name::*} is not set:
				message "{@prefix} 선정된 구역이 없습니다."
				stop
			else:
				message "{@prefix} %{region::name::*}%"
				stop

		else if arg-1 is "delete":
			if arg-2 is not set:
				message "{@prefix} 영역 이름을 적어주세요!"
				stop

			else if {region::name::*} contains arg-2:
				set {_delete} to 0
				loop {region::name::*}:
					if loop-value is arg-2:
						set {_delete} to loop-index
				clear {region::name::%{_delete}%}
				clear {region::x1::%{_delete}%}
				clear {region::x2::%{_delete}%}
				clear {region::y1::%{_delete}%}
				clear {region::y2::%{_delete}%}
				clear {region::z1::%{_delete}%}
				clear {region::z2::%{_delete}%}
				clear {region::world::%{_delete}%}
				message "{@prefix} 해당 영역이 삭제되었습니다."
				stop

			else:
				message "{@prefix} 해당 영역이 존재하지 않습니다!"
				stop

		else if arg-1 is "create":
			if arg-2 is not set:
				message "{@prefix} 구역의 이름을 선택해 주세요."
				stop

			else if {region::%uuid of player%::pos1} is not set:
				message "{@prefix} 첫번째 지점이 선택되지 않았습니다."
				stop

			else if {region::%uuid of player%::pos2} is not set:
				message "{@prefix} 두번째 지점이 선택되지 않았습니다."
				stop

			else if {region::*} contains arg-2:
