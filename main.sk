on shoot:
	set {_world} to world of event-location
	set {_loc} to event-location
	set {_result} to findregion({_world}, {_loc})
	if {_result} is "-1":
		if shooter is player:
			if {global::shoot::%projectile%} is "false":
				cancel event
	else:
		if {region::shoot::%projectile%::%{_result}%} is "false":
			cancel event

on explode:
	send "%event-entity%%{global::explode::%event-entity%}%" to players
	set {_world} to world of event-location
	set {_loc} to event-location
	set {_result} to findregion({_world}, {_loc})
	if {_result} is "-1":
		if event-entity is TNT:
			if {global::explode::TNT} is "false":
				create a safe explosion of force 4 at event-entity
				kill event-entity
		else if {global::explode::%event-entity%} is "false":
			if {global::damage::%event-entity%} is "true":
				create a safe explosion of force 4 at event-entity
				kill event-entity
			else:
				cancel event
				kill event-entity

	else:
		if {region::explode::%event-entity%::%{_result}%} is "false":
			cancel event

on bed enter:
	set {_world} to world of event-location
	set {_loc} to event-location
	set {_result} to findregion({_world}, {_loc})
	if event-world is not "world":
		if {_result} is "-1":
			if {global::explode::bed} is "false":
				cancel event
		else if {region::explode::bed::%{_result}%} is "false":
			cancel event

on spawn:
	set {_ent} to entity
	set {_world} to world of event-location
	set {_loc} to event-location
	set {_result} to findregion({_world}, {_loc})
	if {_result} is "-1":
		if {global::spawn::%{_ent}%} is "false":
			send "%event-entity%-1" to console
			cancel event
	else if {region::spawn::%event-entity%::%{_result}%} is "false":
		cancel event

#on damage of a player:

function findregion(w:world, loc:location) :: text:
	set {_x} to x coord of {_loc}
	set {_y} to y coord of {_loc}
	set {_z} to z coord of {_loc}
	loop {region::world::*}:
		if {_w} is loop-value:
			add loop-index to {_region::*}

	if {_region::*} is set:
		loop {_region::*}:
			if {_x} >= {region::x1::%loop-value%}:
				add loop-value to {_region2::*}
		clear {_region::*}

		if {_region2::*} is set:
			loop {_region2::*}:
				if {_x} <= {region::x2::%loop-value%}:
					add loop-value to {_region::*}
			clear {_region2::*}

			if {_region::*} is set:
				loop {_region::*}:
					if {_y} >= {region::y1::%loop-value%}:
						add loop-value to {_region2::*}
				clear {_region::*}

				if {_region2::*} is set:
					loop {_region2::*}:
						if {_y} <= {region::y2::%loop-value%}:
							add loop-value to {_region::*}
					clear {_region2::*}

					if {_region::*} is set:
						loop {_region::*}:
							if {_z} >= {region::z1::%loop-value%}:
								add loop-value to {_region2::*}
						clear {_region::*}

						if {_region2::*} is set:
							loop {_region2::*}:
								if {_z} <= {region::z2::%loop-value%}:
									add loop-value to {_region::*}
							clear {_region2::*}

							if {_region::*} is set:
								return {_region::1}
							else:
								return "-1"
						else:
							return "-1"
					else:
						return "-1"
				else:
					return "-1"
			else:
				return "-1"
		else:
			return "-1"
	else:
		return "-1"
